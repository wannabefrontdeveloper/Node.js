<form action="/auth/person" method="post">

    <label for="id">아이디:</label><br>
        <input type="text" id="id" name="id" required><br>

    <label for="pwd">비밀번호:</label><br>
        <input type="password" id="pwd" name="pwd" required><br>

    <label for="name">이름:</label><br>
        <input type="text" id="name" name="name" required><br>

    <label for="tel">전화번호:</label><br>
        <input type="text" id="tel" name="tel" required><br>

    <label for="address">주소:</label><br>
        <input type="text" id="address" name="address" required><br> 

    <label for="birth">생년월일:</label><br>
        <input type="text" id="birth" name="birth" required><br> 

    <input type="submit" value="회원가입">

    <label for="class"></label><br>
        <input type="hidden" id="class" name="class" value="02" required><br>

    <label for="point"></label><br>
        <input type="hidden" id="point" name="point" value="0" ><br>
</form>